<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TextField?>
<VBox spacing="20.0" xmlns="http://javafx.com/javafx/18"
  fx:controller="edu.northeast.imageConverter.CalcuController"
  xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane VBox.vgrow="ALWAYS">
      <children>
        <Button layoutX="77.0" layoutY="51.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="101.0" text="+"
          onAction="#onPlus"/>
        <Button layoutX="212.0" layoutY="51.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="101.0" text="-"
          onAction="#onSubtract"/>
        <Button layoutX="77.0" layoutY="95.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="101.0" text="x"
          onAction="#onMultiply"/>
        <Button layoutX="212.0" layoutY="95.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="101.0" text="/"
          onAction="#onDevide"/>
        <Label layoutX="77.0" styleClass="label-section" layoutY="206.0"
          prefHeight="34.0" prefWidth="236.0" fx:id="lblResult"/>
        <Button layoutX="77.0" layoutY="255.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="236.0"
          text="Clear" onAction="#onClear"/>
        <TextField layoutX="77.0" layoutY="151.0" prefHeight="34.0" prefWidth="101.0" fx:id="txtLeft"/>
        <TextField layoutX="212.0" layoutY="151.0" prefHeight="34.0" prefWidth="101.0" fx:id="txtRight"/>
      </children>
    </AnchorPane>
  </children>
  <stylesheets>
    <URL value="@calcu.css"></URL>
  </stylesheets>
</VBox>